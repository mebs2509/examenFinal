@model IEnumerable<Universidad.Entities.Alumno>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EstadoCivil.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genero.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoCivil.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genero.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AlumnoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AlumnoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AlumnoId })
        </td>
    </tr>
}

</table>
